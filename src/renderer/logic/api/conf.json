[
  {
    "slug": "wc",
    "default": true,
    "requester": "oauth",
    "config": {
      "authentication": "oauth",
      "version": "1.0a",
      "type": "one_legged",
      "algorithm": "HMAC-SHA1",
      "authQuery": true,
      "authHeader": false,
      "indexArrays": true,
      "addDataToQuery": true,
      "timestampLength": 10,
      "taleNonce": "",
      "put": {
        "override": {
          "arg": "_method",
          "method": "POST"
        }
      },
      "delete": {
        "authQuery": true,
        "override": {
          "arg": "_method",
          "method": "POST"
        }
      }
    },
    "mappings": {
      "index": {
        "endpoint": "/wp-json/wc/v2/",
        "batch": {
          "save": "update"
        }
      },
      "auth": {
        "endpoint": "/wc-auth/v1/authorize",
        "params": [
          {
            "key": "app_name",
            "value": "storepilot"
          },
          {
            "key": "scope",
            "value": "read_write"
          },
          {
            "key": "user_id",
            "value": "1"
          },
          {
            "key": "return_url",
            "value": "https://proxy.output.one/tokens/my_token.php"
          },
          {
            "key": "callback_url",
            "value": "https://proxy.output.one?redirect_token=my_token"
          }
        ],
        "batch": {
          "save": "update"
        }
      },
      "product": {
        "endpoint": "/wp-json/wc/v2/products{/id}{/batch}",
        "batch": {
          "save": "update"
        },
        "identifier": "id",
        "creationIdentifier": "meta_data=[{\"key\": \"sp_batch_create_id\", \"value\": \"identifier\" }]"
      },
      "review": {
        "endpoint": "/wp-json/wc/v2/products/{parent_id}/reviews{/id}",
        "batch": {
          "save": "update"
        },
        "identifier": "id",
        "creationIdentifier": "meta_data=[{\"key\": \"sp_batch_create_id\", \"value\": \"identifier\" }]"
      },
      "reviews": {
        "endpoint": "/wp-json/wc/v2/products/{parent_id}/reviews{/batch}",
        "multiple": true,
        "child": "review",
        "batch": {
          "save": "update"
        }
      },
      "productTag": {
        "endpoint": "/wp-json/wc/v2/products/tags{/id}",
        "batch": {
