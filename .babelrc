{
  "presets": [
    ["env", { "modules": false, "exclude": ["transform-es2015-classes"] }],
    "stage-2"
  ],
  "plugins": [
    "transform-runtime",
    [
      "component", [{
      "libraryName": "element-ui",
      "styleLibraryName": "theme-chalk"
    }]
    ]
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [ "istanbul" ]
    }
  }
}
